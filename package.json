{
  "name": "api-particulier",
  "version": "3.0.0",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "test": "mocha \"src/**/*.test.ts\"",
    "coverage": "nyc --reporter=lcov mocha \"src/**/*.test.ts\"",
    "test:integration": "mocha -r dotenv/config -r test/integration/database-register.js \"test/integration/**/*.test.ts\"",
    "test:end-to-end": "npx cypress open",
    "lint": "gts lint",
    "clean": "gts clean",
    "type-check": "tsc --noEmit",
    "compile": "babel src --out-dir build --extensions \".ts,.js\" --copy-files",
    "migrate": "ts-node -r dotenv/config node_modules/.bin/node-pg-migrate -j ts",
    "build": "run-p build:*",
    "build:backend": "run-p type-check compile",
    "build:frontend": "vite build",
    "start": "node build/infrastructure/server.js",
    "start:dev": "run-p start:dev:*",
    "start:dev:backend": "ts-node-dev --watch .env --files -r dotenv/config -r tsconfig-paths/register src/infrastructure/server.ts",
    "start:dev:frontend": "vite --host",
    "fixtures:load": "ts-node -r dotenv/config -r tsconfig-paths/register src/presentation/cli/load-fixtures.ts"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.1.0",
    "@hotwired/turbo": "^7.0.0-rc.2",
    "@sentry/node": "^6.10.0",
