    "autoprefixer": "^10.3.1",
    "axios-mock-adapter": "^1.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "bull-repl": "^0.26.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cssnano": "^5.0.7",
    "cypress": "^8.3.0",
    "esbuild": "^0.12.19",
    "eslint-plugin-cypress": "^2.11.3",
    "gts": "^3.1.0",
    "gulp": "^4.0.0",
    "gulp-esbuild": "^0.8.5",
    "gulp-postcss": "^9.0.0",
