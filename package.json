{
  "name": "api-particulier",
  "version": "3.0.0",
  "scripts": {
    "test": "mocha \"src/**/*.test.ts\"",
    "coverage": "nyc --reporter=lcov mocha \"src/**/*.test.ts\"",
    "test:integration": "mocha -r dotenv/config -r test/integration/database-register.js \"test/**/*.test.ts\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "type-check": "tsc --noEmit",
    "compile": "babel src --out-dir build --extensions \".ts,.js\" --copy-files",
    "fix": "gts fix",
    "migrate": "ts-node -r dotenv/config node_modules/.bin/node-pg-migrate -j ts",
    "build": "npm run type-check && npm run compile",
    "start:dev": "ts-node-dev -r dotenv/config -r tsconfig-paths/register src/infrastructure/server.ts",
    "start": "node build/infrastructure/server.js",
    "start:worker:dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/infrastructure/worker.ts",
    "start:worker": "node -r dotenv/config build/infrastructure/worker.js",
    "migrate-gravitee-applications": "ts-node -r tsconfig-paths/register src/presentation/cli/migrate-gravitee-applications.ts",
    "publish-all-events": "ts-node -r tsconfig-paths/register src/presentation/cli/publish-all-events.ts",
    "bundle-frontend": "esbuild  src/presentation/frontend/index.ts --bundle --outfile=public/out.js --minify --sourcemap --target=chrome58,firefox57,safari11,edge16"
  },
  "dependencies": {
    "@bull-board/api": "^3.4.0",
    "@bull-board/express": "^3.4.0",
    "@gouvfr/dsfr": "^1.1.0",
    "@sentry/node": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "axios": "^0.21.1",
    "axios-oauth-client": "^1.4.3",
    "axios-token-interceptor": "^0.2.0",
    "body-parser": "^1.19.0",
    "bullmq": "^1.38.1",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.9",
    "commander": "^4.1.1",
    "core-js": "^3.15.2",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fast-xml-parser": "^3.19.0",
    "ioredis": "^4.27.6",
    "lodash": "^4.17.21",
    "node-pg-migrate": "^5.10.0",
    "openapi3-ts": "^2.0.1",
    "pg": "^8.6.0",
    "qs": "^6.7.0",
    "query-string": "^7.0.0",
    "stimulus": "^2.0.0",
    "tailwindcss": "^2.2.7",
    "uuid": "^8.3.2",
    "xmldom": "^0.6.0",
    "xpath": "0.0.32",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/axios-token-interceptor": "^0.2.2",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cheerio": "^0.22.29",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.1",
    "@types/xmldom": "^0.1.30",
    "axios-mock-adapter": "^1.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "brunch": "^2.10.17",
    "bull-repl": "^0.26.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "esbuild": "^0.12.19",
    "gts": "^3.1.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "ts-sinon": "^2.0.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.2",
    "typescript-brunch": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/api-particulier-monolith.git"
  },
  "bugs": {
    "url": "https://github.com/betagouv/api-particulier-monolith/issues"
  },
  "homepage": "https://github.com/betagouv/api-particulier-monolith#readme"
}
