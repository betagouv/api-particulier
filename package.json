{
  "name": "api-particulier",
  "version": "3.0.0",
  "scripts": {
    "test": "mocha \"src/**/*.test.ts\"",
    "coverage": "nyc --reporter=lcov mocha \"src/**/*.test.ts\"",
    "test:integration": "mocha -r dotenv/config -r test/integration/database-register.js \"test/integration/**/*.test.ts\"",
    "lint": "gts lint",
    "clean": "gts clean",
    "type-check": "tsc --noEmit",
    "compile": "babel src --out-dir build --extensions \".ts,.js\" --copy-files",
    "migrate": "ts-node -r dotenv/config node_modules/.bin/node-pg-migrate -j ts",
    "build": "run-p build:*",
    "build:backend": "run-p type-check compile",
    "build:frontend": "gulp build",
    "start": "node build/infrastructure/server.js",
    "start:dev:backend": "ts-node-dev -r dotenv/config -r tsconfig-paths/register src/infrastructure/server.ts",
    "start:dev:frontend": "gulp dev",
    "migrate-gravitee-applications": "ts-node -r tsconfig-paths/register src/presentation/cli/migrate-gravitee-applications.ts",
    "publish-all-events": "ts-node -r tsconfig-paths/register src/presentation/cli/publish-all-events.ts"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.1.0",
    "@sentry/node": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@xmldom/xmldom": "^0.7.1",
    "axios": "^0.21.1",
    "axios-oauth-client": "^1.4.3",
    "axios-token-interceptor": "^0.2.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.9",
    "chokidar": "^3.5.2",
    "commander": "^4.1.1",
    "connect-redis": "^6.0.0",
    "core-js": "^3.15.2",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-session": "^1.17.2",
    "fast-xml-parser": "^3.19.0",
    "ioredis": "^4.27.6",
    "lodash": "^4.17.21",
    "node-pg-migrate": "^5.10.0",
    "nunjucks": "^3.2.3",
    "openapi3-ts": "^2.0.1",
    "openid-client": "^4.7.4",
    "passport": "^0.4.1",
    "pg": "^8.6.0",
    "qs": "^6.7.0",
    "query-string": "^7.0.0",
    "stimulus": "^2.0.0",
    "uuid": "^8.3.2",
    "vhost": "^3.0.2",
    "xpath": "0.0.32",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@types/axios": "^0.14.0",
    "@types/axios-token-interceptor": "^0.2.2",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cheerio": "^0.22.29",
    "@types/connect-redis": "^0.0.17",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
