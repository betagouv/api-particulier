    "@xmldom/xmldom": "^0.7.1",
    "axios": "^0.21.2",
    "axios-oauth-client": "^1.4.3",
    "axios-token-interceptor": "^0.2.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "cheerio": "^1.0.0-rc.9",
    "chokidar": "^3.5.2",
    "commander": "^4.1.1",
    "connect-redis": "^6.0.0",
    "cookie-parser": "1.4.4",
    "core-js": "^3.15.2",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "debug": "~4.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-flash-message": "^2.1.0",
    "express-jwt": "6.0.0",
    "express-session": "^1.17.2",
    "fast-xml-parser": "^3.19.0",
    "forest-express-sequelize": "^8.0.0",
